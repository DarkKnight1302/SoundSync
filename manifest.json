{
  "name": "Sound Sync",
  "description": " Extension to share your webpage across nearby devices through sound.",
  "offline_enabled": false,
  "version": "1.0.2",
  "manifest_version": 2,
  "default_locale": "en",
  "background": {
    "scripts": ["js/libraries/ponyfill.min.js", "js/libraries/StreamSaver.min.js", "js/background.js"],
    "persistent": false
  },

  "browser_action": {
    "default_icon": "assets/extension-icons/sound-32.png",
      "default_popup": "html/popup.html"
  },
  "icons": {
    "16": "assets/extension-icons/sound-16.png",
    "128": "assets/extension-icons/sound-128.png",
    "48": "assets/extension-icons/sound-48.png"
  },
  "content_scripts": [
      {
          "matches": [
          "http://*/*", "https://*/*"
          ],
          "js": ["js/detect.js", "js/ggwave.js", "js/ggwaveRec.js"]
      }
  ],
  "chrome_url_overrides" : {
    "newtab": "html/contentinject.html"
  },
	"content_security_policy": "script-src 'self' 'unsafe-eval' chrome-extension://kbbdabhdfibnancpjfhlkhafgdilcnji/js/libraries/ffmpeg.js 'unsafe-inline'; object-src 'self'",
  "permissions": [
    "activeTab", "tabCapture", "storage"
  ],
  "web_accessible_resources": [        
    "html/camera.html","html/sources.html","html/settings.html","js/content.js","js/ggwave.js","js/ggwaveRec.js","js/libraries/ffmpeg.js", "css/content.css","assets/images/*","js/sources.js", "js/cameracontent.js", "css/cameracontent.css", "js/libraries/plyr.min.js", "js/libraries/StreamSaver.min.js", "css/libraries/plyr.min.css", "js/audiosources.js", "html/audiosources.html"
  ],
  "commands": {}
}

